{
  "name": "@gasbuddy/starter-kit-web",
  "version": "2.0.0",
  "description": "A shell project for a GasBuddy web application",
  "private": true,
  "scripts": {
    "start": "build/index.js",
    "start-dev": "nodemon -r babel-register src/index.js",
    "build": "npm run build:client && npm run build:server",
    "postinstall": "npm run build",
    "build:client": "webpack --env.production --config config/webpack.config.js",
    "build:visualize": "cross-env WEBPACK_VISUALIZE=true webpack --env.production --config config/webpack.config.js",
    "build:server": "cross-env NODE_ENV=production babel --source-maps inline -Dd build/ src/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "keywords": [
    "react",
    "web",
    "gasbuddy"
  ],
  "author": "GasBuddy <developers@gasbuddy.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@gasbuddy/react": "^1.0.0",
    "@gasbuddy/service": "^3.3.0",
    "bluebird": "^3.5.0",
    "connect-mongo": "^1.3.2",
    "express-session": "^1.15.2",
    "minimist": "^1.2.0",
    "mongoose": "^4.9.8",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-ace": "^4.2.1",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.2.3",
    "react-redux": "^5.0.3",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0",
    "semantic-ui-css": "^2.2.9",
    "semantic-ui-react": "^0.68.3",
    "source-map-support": "^0.4.15",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@gasbuddy/web-dev": "^1.0.0",
    "babel-cli": "^6.9.0",
    "babel-preset-gasbuddy": "^2.1.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.10.2",
    "eslint-config-gasbuddy": "^2",
    "webpack": "^2.3.3"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
